{
  "name": "tennis-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@hookstate/core": "^4.0.1",
    "@hookstate/localstored": "^4.0.2",
    "@material/menu": "^14.0.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.4",
    "buffer": "^6.0.3",
    "date-fns": "^2.28.0",
    "enzyme": "^3.11.0",
    "highcharts": "^9.3.2",
    "highcharts-react-official": "^3.1.0",
    "html2canvas": "^1.4.1",
    "humanize-duration": "^3.27.2",
    "i18next": "^21.8.10",
    "jspdf": "^2.5.1",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.2.0",
    "react-gtm-module": "^2.0.11",
    "react-query": "^3.39.0",
    "react-responsive": "^9.0.0",
    "react-router-dom": "6",
    "recharts": "^2.1.13",
    "web-vitals": "^2.1.3"
  },
  "scripts": {
    "start": "CHOKIDAR_USEPOLLING=true WATCHPACK_POLLING=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:quiet": "npm run lint -- --quiet",
    "lint:fix": "npm run lint -- --fix",
    "coverage": "react-scripts test --coverage --watchAll=false",
    "test:nowatch": "react-scripts test --watchAll=false",
    "tennisapi:update": "openapi --useUnionTypes --useOptions --name TennisApi -i openapi/public-api.yaml -o src/data/openapi",
    "prettier:check": "prettier --check src",
    "prettier:fix": "prettier --write src"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/humanize-duration": "^3.27.1",
    "@types/jest": "^28.1.2",
    "@types/node": "^18.0.0",
    "@types/node-fetch": "^2.6.9",
    "@types/react": "^18.0.14",
    "@types/react-datepicker": "^4.4.2",
    "@types/react-dom": "^18.0.5",
    "@types/react-gtm-module": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-no-relative-import-paths": "^1.3.3",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest-junit": "^13.2.0",
    "openapi-typescript": "^5.4.0",
    "openapi-typescript-codegen": "^0.23.0",
    "prettier": "^2.7.1",
    "react-scripts": "^5.0.1",
    "reduce-css-calc": "^2.1.8",
    "sass": "^1.52.3",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-prettier": "^2.0.0",
    "type-fest": "^2.12.2",
    "typescript": "^4.7.4"
  },
  "jest": {
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover",
      "cobertura"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!d3|d3-array|internmap|delaunator|robust-predicates)"
    ]
  },
  "jest-junit": {
    "reportTestSuiteErrors": "true"
  }
}
